@inherits BaseStyledComponent
@using Microsoft.AspNetCore.Components.Forms

@if (Visible)
{
	<InputFile id="@ClientID"
			   multiple="@AllowMultiple"
			   accept="@Accept"
			   style="@Style"
			   class="@CssClass"
			   disabled="@(!Enabled)"
			   title="@ToolTip"
			   OnChange="OnFileChangeInternal" />
}
