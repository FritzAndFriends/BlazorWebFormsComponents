@inherits ButtonBaseComponent

@if (Visible)
{
	if (Enabled)
	{
		<input type="image" src="@ImageUrl" alt="@AlternateText" longdesc="@DescriptionUrl" style="@CalculatedStyle" class="@CssClass" @onclick="Click" />
	}
	else
	{
		<input type="image" src="@ImageUrl" alt="@AlternateText" longdesc="@DescriptionUrl" style="@CalculatedStyle" class="@CssClass" disabled="disabled" @onclick="Click" />
	}
}
