@inherits ComponentBase
@implements IDisposable
@if (!string.IsNullOrEmpty(_currentTitle))
{
    <PageTitle>@_currentTitle</PageTitle>
}
@if (!string.IsNullOrEmpty(_currentMetaDescription))
{
<HeadContent>
    <meta name="description" content="@_currentMetaDescription" />
</HeadContent>
}
@if (!string.IsNullOrEmpty(_currentMetaKeywords))
{
<HeadContent>
    <meta name="keywords" content="@_currentMetaKeywords" />
</HeadContent>
}
