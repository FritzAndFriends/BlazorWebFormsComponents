@inherits ButtonBaseComponent

@if (Visible)
{
<input type="@CalculatedButtonType"
				id="@ClientID"
				@onclick="Click"
				OnClientClick="@OnClientClick"
				style="@Style"
				class="@CalculatedCssClass"
				disabled="@(Enabled ? null : "disabled")"
				title="@ToolTip"
				accesskey="@(string.IsNullOrEmpty(AccessKey) ? null : AccessKey)"
				value="@Text" />
}
